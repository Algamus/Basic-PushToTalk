<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gokhangobus.projectpushtotalk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="gokhangobus.projectpushtotalk.SettingPTT" android:label="SettingPTT">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category android:name="gokhangobus.projectpushtotalk.SettingPTT"/>
            </intent-filter>
        </activity>

        <activity android:name="gokhangobus.projectpushtotalk.Ptt" android:label="Ptt">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category android:name="gokhangobus.projectpushtotalk.Ptt"/>
            </intent-filter>
        </activity>




    </application>

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />


</manifest>
